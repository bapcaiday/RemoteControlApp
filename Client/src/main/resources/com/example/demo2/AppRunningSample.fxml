<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.effect.DropShadow?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" style="-fx-background-color: #263F73;" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.demo2.AppRunningPController">
    <children>
        <Button layoutX="33.0" layoutY="182.0" mnemonicParsing="false" onAction="#moveToKillApp" prefHeight="73.0" prefWidth="110.0" text="Kill">
         <font>
            <Font size="18.0" />
         </font></Button>
        <Button layoutX="33.0" layoutY="68.0" mnemonicParsing="false" onAction="#getListRunningApp" prefHeight="73.0" prefWidth="110.0" text="Get list">
         <font>
            <Font size="18.0" />
         </font></Button>
        <Button layoutX="33.0" layoutY="296.0" mnemonicParsing="false" onAction="#moveToStartApp" prefHeight="73.0" prefWidth="110.0" text="Start">
         <font>
            <Font size="18.0" />
         </font></Button>
        <TableView fx:id="ListAppTable" layoutX="214.0" layoutY="68.0" prefHeight="301.0" prefWidth="341.0">
            <columns>
                <TableColumn fx:id="nameApp" prefWidth="243.0" text="Name App" />
                <TableColumn fx:id="pidApp" prefWidth="96.0" text="PID" />
            </columns>
        </TableView>
      <Label layoutX="306.0" layoutY="22.0" text="APPLICATION" textFill="WHITE">
         <font>
            <Font name="System Bold" size="24.0" />
         </font>
         <effect>
            <DropShadow spread="0.59" />
         </effect>
      </Label>
    </children>
</AnchorPane>
